<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xie.teacher_evalu.persistence.AdminAccountMapper">
    <select id="getAdminAccountByIdAndPassword" parameterType="string" resultType="AdminAccount">
        select admin_id,password
        from  admin_account
        where admin_id = #{arg0} and password= #{arg1}
    </select>

















</mapper>
